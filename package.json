{
  "name": "pdf-reader",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "pdfjs-dist": "^2.13.216",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.39.4",
    "react-modal": "^3.16.1",
    "react-pdf": "^5.7.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "pdf.reader",
    "windows": {
      "category": "pdf.reader"
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "concurrently \"yarn start\" \"electron .\"",
    "electron:build": "electron-packager .",
    "electron:test": "electron .",
    "electron:prod": "concurrently cross-env PROD=5000 \"serve -s build\" & \"electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-modal": "^3.13.1",
    "@types/react-pdf": "^5.0.9",
    "concurrently": "^7.0.0",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^15.5.1",
    "wait-on": "^6.0.1",
    "worker-loader": "^3.0.8"
  }
}
